            __x1__ = (+ __x1__ __x3__)
            __x2__ = (+ __x2__ 1i)
          }
          let S = __x1__
        }
      } else {
        if (|| (== n 1i) (< 4i n)) throw INTRINSIC_URIError else {}
        if (! (< (+ k (* 3i (- n 1i))) strLen)) throw INTRINSIC_URIError else {}
        let Octets = (new [B])
        let j = 1i
        while (< j n) {
          k = (+ k 1i)
          if (! (= string[k] "%")) throw INTRINSIC_URIError else {}
          ??? "If the code units at index ( id:{k} + 1 ) and ( id:{k} + 2 ) within id:{string} do not represent hexadecimal digits , throw a value:{URIError} exception ."
