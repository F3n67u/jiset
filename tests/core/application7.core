let sum = (bias, ...list) => {
    let res = bias
    let i = i0
    while (< i list.length) {
        res = (+ res list[i])
        i = (+ i i1)
    }
    return res
}

let sum2 = (...list) => {
    let res = i0
    let i = i0
    while (< i list.length) {
        res = (+ res list[i])
        i = (+ i i1)
    }
    return res
}

let x = (sum i0 i1 i2 i3 i4)
let y = (sum2 i0 i1 i2 i3 i4)
assert (= x i10)
assert (= y i10)
