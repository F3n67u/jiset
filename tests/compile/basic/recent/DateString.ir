{
  app __x0__ = (Type tv)
  assert (= __x0__ Number)
  assert (! (= tv NaN))
  ??? "Let id:{weekday} be the Name of the entry in link:{unhandled: sec-todatestring-day-names} with the Number WeekDay ( id:{tv} ) ."
  ??? "Let id:{month} be the Name of the entry in link:{unhandled: sec-todatestring-month-names} with the Number MonthFromTime ( id:{tv} ) ."
  ??? "Let id:{day} be the String representation of DateFromTime ( id:{tv} ) , formatted as a two - digit decimal number , padded to the left with the code unit 0x0030 ( DIGIT ZERO ) if necessary ."
  app __x1__ = (YearFromTime tv)
  let yv = __x1__
  if (! (< yv 0i)) let yearSign = "" else let yearSign = "-"
  ??? "Let id:{year} be the String representation of abs ( ℝ ( id:{yv} ) ) , formatted as a decimal number ."
  app __x2__ = (StringPad year 4i "0" CONST_start)
  let paddedYear = [! __x2__]
  return (+ (+ (+ (+ (+ (+ (+ weekday " ") month) " ") day) " ") yearSign) paddedYear)
}