let sum = (bias, ...list) => {
    let res = bias
    let i = 0i
    while (< i (length-of list)) {
        res = (+ res list[i])
        i = (+ i 1i)
    }
    return res
}

let sum2 = (...list) => {
    let res = 0i
    let i = 0i
    while (< i (length-of list)) {
        res = (+ res list[i])
        i = (+ i 1i)
    }
    return res
}

let x = (sum 0i 1i 2i 3i 4i)
let y = (sum2 0i 1i 2i 3i 4i)
assert (= x 10i)
assert (= y 10i)
